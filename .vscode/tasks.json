{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Debug",
			"type": "shell",
			"command": "msbuild",
			"args": [
				"/property:Configuration=Debug",
				"/property:GenerateFullPaths=true",
				"/t:Rebuild",
				"-m",
				"-clp:ShowTimestamp"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"reveal": "silent"
			},
			"problemMatcher": "$msCompile"
		},
		{
			"label": "Build Release",
			"type": "shell",
			"command": "msbuild",
			"args": [
				"/property:Configuration=Release",
				"/property:GenerateFullPaths=true",
				"/t:Rebuild",
				"-m",
				"-clp:ShowTimestamp"
			],
			"group": "build",
			"presentation": {
				"reveal": "silent"
			},
			"problemMatcher": "$msCompile"
		}
	]
}